{$lua}
local rAdd = getAddress('PlayerBase+Rotation')
local Rotation = readFloat(rAdd)
local RotationMax = Rotation+360

function Spin(spin)
    if isKeyPressed(17) and isKeyPressed(112) then
      spin.destroy()
    end
    if Rotation &lt; RotationMax then
        Rotation = Rotation+10
    else
        Rotation = Rotation-360
    end
    writeFloat(rAdd, Rotation)
end

function ActivateSpin()
  spin=createTimer(nil)
  spin.OnTimer=Spin
  spin.Interval=1
  spin.Enabled=true
end
{$asm}
luacall(ActivateSpin())
[disable]
luacall(spin.destroy())
