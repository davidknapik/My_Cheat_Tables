autoAssemble([[
aobscanmodule(GameBase,cubeworld.exe,48 8B 0D * * * * F2 0F 5E C8 F2 0F)
registersymbol(GameBase)
]])
local address = getAddress("GameBase") + 3
address = address + readInteger(address) + 4
unregisterSymbol("GameBase")
registerSymbol("GameBase", address, true)
unregisterSymbol("PlayerBase")
registerSymbol("PlayerBase", "[[[[[GameBase]+8]+40]+0]+10]+0", true)
unregisterSymbol("WorldBase")
registerSymbol("WorldBase", "[GameBase]+0", true)
{$asm}
[DISABLE]
unregistersymbol(GameBase)
unregistersymbol(PlayerBase)
unregistersymbol(WorldBase)
