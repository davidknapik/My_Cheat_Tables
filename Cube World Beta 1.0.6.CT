{$lua}
local fAdd = getAddress('PlayerBase+Flag')
local vAdd = getAddress('PlayerBase+Velocity.z')
local fVal = readBytes(fAdd, 1)
--print('Flag :', fVal  &amp; 0x01)
function Bhop(bhop)
  if isKeyPressed(17) and isKeyPressed(112) then
    bhop.destroy()
  end
  if isKeyPressed(32) and (readBytes(fAdd, 1)&amp; 0x01) == 1 then
        writeFloat(vAdd, 12.0)
  end
end

function ActivateBhop()
  bhop=createTimer(nil)
  bhop.OnTimer=Bhop
  bhop.Interval=1
  bhop.Enabled=true
end
{$asm}
luacall(ActivateBhop())
[disable]
luacall(bhop.destroy())
